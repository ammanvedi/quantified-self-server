{
  "compilerOptions": {
    "lib": [
      "es5",
      "es6",
      "es2018.asynciterable",
      "dom"
    ],
    "target": "es6",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./dist",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "esModuleInterop": true
  },
  "exclude": ["src/**/*.test.ts"],
  "include": ["src/**/*"]
}